{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2015",
    "module": "CommonJS",
    "noEmit": true,
    "declaration": false,
    "importHelpers": true,
    "incremental": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "jsx": "preserve"
  },
  "include": [
    ".gatsby/*.ts",
    "gatsby-*.js"
  ],
  "exclude": [
    "src/**/*",
    "node_modules/**/*",
  ]
}